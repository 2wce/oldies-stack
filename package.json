    "vitest": "^0.30.1"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register database/seeders/index.ts",
    "schema": "database/schema.prisma"
  }
}
